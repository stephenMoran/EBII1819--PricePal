{
  "name": "PricePal",
  "description": "Tool that compares prices on Amazon with other well trusted retailers",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
     "matches": ["http://www.amazon.com/*"],
     "js": ["content.js"]
    }
  ]
}
